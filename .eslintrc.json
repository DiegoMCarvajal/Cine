{
    "extends": [
        "eslint:recommended",
        "pluging:import/errors",
        "pluging:react/recommended",
        "pluging:jsx-a11y/recommended",
        "pluging:react-hooks/recommended",
        "prettier"
    ],
    "rules": {
        "react/prop-types" : 0,
        "react/react-in-jsx-scope":0
    },
    "plugins": [
        "react","import","jsx-a11y", "react-hooks"
    ],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures":{
            "jsx" : true
        }
    },
    "env" : {
    "es6" : true,
    "browser" : true,
    "node" : true
    },
    "settings": {
        "react":{
            "version": "17.0.2" 
        }
    }
}